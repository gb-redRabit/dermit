{
  "name": "dermit",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "productName": "Dermit",
  "author": "author",
  "license": "UNLICENSED",
  "description ": "description",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "electron-fetch": "^1.9.1",
    "jquery": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
    "swiper": "^10.0.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none react-scripts start\" \"wait-on http://127.0.0.1:3000 && electron .\" -k",
    "bulid": "react-scripts build && electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never"
  },
  "build": {
    "files": [
      "build/**/*",
      "src/**/*",
      "public/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraFiles": [
      "credentials"
    ],
    "publish": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "concurrently": "^8.2.0",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "electron-is-dev": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "wait-on": "^7.0.1"
  },
  "description": "This project was bootstrapped with [Electron],[Create-React-App],[cross-env],[concurrently],[electron-is-dev],[electron-builder],[react-router-dom]"
}
